{
  "name": "@vunix/default-preset-tailwind",
  "version": "1.0.0",
  "description": "Default Vunix preset for tailwindcss",
  "author": "Gaetan SENN",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "changelog": "standard-version"
  },
  "devDependencies": {
    "unbuild": "^1.1.2",
    "@vunix/core": "*"
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    },
    "tag-prefix": "@vunix/default-preset-tailwind@"
  }
}
